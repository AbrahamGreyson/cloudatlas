  "require": {
    "php": ">=5.5",
    "guzzlehttp/guzzle": "~5.3|~6.0.1|~6.1",
    "guzzlehttp/psr7": "~1.0",
    "guzzlehttp/promises": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0|~5.0",
    "behat/behat": "~3.0"
  },
  "autoload": {
    "psr-4": {
